{
    "version": "0.2.0",
    "configurations": [    
        {
            "name": "Launch TUMAINI (Python HTTP)",
            "type": "debugpy",
            "request": "launch",
            "program": "-m",
            "args": ["http.server", "8080"],
            "console": "integratedTerminal"
          },

        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}